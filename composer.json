{
    "name": "spinon/uccello-archive",
    "description": "Archivage d'objets",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandre Giroud",
            "email": "alexandre@spin-on.fr"
        }
    ],
    "require": {
        "uccello/uccello": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "Spinon\\UccelloArchive\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spinon\\UccelloArchive\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spinon\\UccelloArchive\\Providers\\AppServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
